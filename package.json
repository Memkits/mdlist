{
  "name": "calcit-workflow",
  "version": "0.1.0",
  "description": "ClojureScript workflow using Calcit Editor",
  "main": "index.js",
  "scripts": {
    "watch": "shadow-cljs watch client cli",
    "serve": "http-server dist -s",
    "build-cli": "shadow-cljs release cli",
    "build-cdn": "shadow-cljs clj-run build.main/build-cdn",
    "build": "shadow-cljs clj-run build.main/build",
    "upload": "node target/upload.js",
    "up2site": "yarn build-cdn && yarn upload",
    "page": "shadow-cljs clj-run build.main/page",
    "ln": "cd target && rm -f entry && ln -s ../entry",
    "up": "cd api-folder/ && git pull && cd - && node dist/cli.js && yarn build",
    "up-all": "rm -rf dist/* && yarn build-cli && yarn update"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mvc-works/calcit-workflow.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "devDependencies": {
    "copy-text-to-clipboard": "^2.0.0",
    "feather-icons": "^4.19.0",
    "http-server": "^0.11.1",
    "shadow-cljs": "^2.8.23"
  },
  "dependencies": {
    "dayjs": "^1.8.11",
    "fuzzy": "^0.1.3",
    "shortid": "^2.2.14"
  }
}
